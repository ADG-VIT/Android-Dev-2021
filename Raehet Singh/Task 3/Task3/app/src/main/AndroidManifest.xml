<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.task3">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Task3">
        <activity android:name=".App.Authentication.Home"></activity>
        <activity android:name=".App.Authentication.FacebookLogin" />
        <activity android:name=".App.Authentication.GoogleLogin" />
        <activity android:name=".App.Authentication.EmailRegister" />
        <activity android:name=".App.Authentication.EmailLogin" />
        <activity android:name=".App.Authentication.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>